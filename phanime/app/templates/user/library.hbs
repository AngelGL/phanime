<table class="table">
	<thead style="border-bottom: 3px solid #ccc">
		<tr>
			<td>Anime</td>
			<td>Progress</td>
			<td>Rating</td>
			<td>Type</td>
	</thead>

	<tbody>
	{{#each model.library_entries}}
		<tr>
			<td>{{anime.title}}</td>
			<td>{{status}}</td>
			<td>{{#if score}}{{score}}{{else}} - {{/if}}</td>
			<td>{{anime.type}}</td>
		</tr>
	{{/each}}
	</tbody>

</table>